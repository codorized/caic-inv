    include ../components/stage_footer.pug
    script(src='../../js/stages/oncheckup.js')
    script(src='../../js/common.js')
    script(src='../../js/image-conversion.js')
    script. 
        const motorobj = !{JSON.stringify(motorobj)};
        const tagID = motorobj.tagID;
        const oncheckup = !{JSON.stringify(oncheckup)};

        var baseurl = window.location.protocol + "//" + window.location.host + "/";
        - $(document).ready(function() {
            /*Check files to know if they exist*/
            
            //Hide alerts first
            $('.filenotfound').hide();
            
            //Check all file's existence
            fileChecker('oncheckup', tagID, {
              'iarfront_img': 'iar1',
              'iarback_img': 'iar2',
              'iarfront_pdf': 'iarfrontpdf',
              'iarback_pdf': 'iarbackpdf', 
              'bef_img': 'beforeimage'
            });

            //Upload
            $('#file-spinner').hide()
            $('.file-btns').on('click', function(){
              var file_input = $(this).parent().find('input');
              var filename = file_input.attr('name');
              var this_object = $(this);

              if(document.getElementById('file-'+filename).files.length > 0)
              {
                var formData = new FormData();
                var fileObj = document.getElementById('file-'+filename).files[0];
                var fileType = fileObj['type'];
                var validImageTypes = ["image/jpeg", "image/png"];
                var fileSize = fileObj.size/1000;
                
                
                if($.inArray(fileType, validImageTypes) >= 0 && fileSize < 15000){
                  $('.file-btns').prop('disabled', true);
                  $('#file-spinner').show()
                  imageConversion.compressAccurately(fileObj,900).then(res=>{
                    formData.append(filename, res);

                    $.ajax({   
                      async: true, 
                      method: 'POST',
                      data: formData,
                      url: baseurl+'io/uploadFile/'+tagID+'/oncheckup/'+filename+'/old',    
                      processData: false,
                      contentType: false,            
                      success: function (data) {  
                        $('.file-btns').prop('disabled', false);
                        $('#file-spinner').hide()
                        
                        var download_obj = this_object.parent().parent().find('.downloadfile')
                        var notFound_obj = this_object.parent().parent().find('.filenotfound')
                        download_obj.show()
                        notFound_obj.hide()
                       
                      },
                      error: function (xhr, text, error) {              
                        alert(error);
                      }
                    })
                  })
                }
                else 
                {
                  alert('Invalid file type! Must be a JPG or PNG and the file size must not exceed 15 MB')
                  
                }
              }
              else {
                alert('Please select an input!')
              }

          })

            if ( window.history.replaceState ) {
              window.history.replaceState( null, null, window.location.href );
            }
            
            $('.downloadIAR').on('click', function(){
                window.open(baseurl+'io/download')
            })

            //Fill out ordinary text fields
            $('input[type="text"]').each(function( index ) {
              var name = $(this).prop('name')
               if(name == 'power'){
                  $(this).val(oncheckup[name].split(' ')[0]);
                  if(oncheckup[name].split(' ')[1] == 'HP') $('input[value="hp"]').prop('checked', true)
                  else $('input[value="kw"]').prop('checked', true)
                }
                else 
                  $(this).val(oncheckup[name]);
            })

             //Fillout date
            var now = new Date(oncheckup.startdate); 
            var day = ("0" + now.getDate()).slice(-2);
            var month = ("0" + (now.getMonth() + 1)).slice(-2);
            var startdate = now.getFullYear()+"-"+(month)+"-"+(day) ;

            var now2 = new Date(oncheckup.finishdate); 
            var day2 = ("0" + now2.getDate()).slice(-2);
            var month2 = ("0" + (now2.getMonth() + 1)).slice(-2);
            var finishdate = now2.getFullYear()+"-"+(month2)+"-"+(day2) ;

            $('input[name="startdate"]').val(startdate);
            $('input[name="finishdate"]').val(finishdate);


            //Fill out each scope
            for(var i=0; i<oncheckup.stator.length;i++)
            {
                $('input[type="checkbox"]').each(function( index ) {
                    var name = $(this).prop('name')
                    if(oncheckup.stator[i].name == name) {
                      $(this).prop('checked', true)
                    }; 
                })
                $('input[type="radio"]').each(function( index ) {
                    var name = $(this).prop('name')
                    if(name == 'stator-complete'){
                        if($(this).val() == 'complete-rewinding' && oncheckup.stator[i].name == 'stator-complete-rewinding') {$(this).prop('checked', true)}
                        else if($(this).val() == 'complete-reconditioning' && oncheckup.stator[i].name == 'stator-complete-recon') {$(this).prop('checked', true)};
                    }
                })
            }

            for(var i=0; i<oncheckup.accessories.length;i++)
            {
                 $('input[type="text"]').each(function( index ) {
                    var name = $(this).prop('name')
                    name = name.replace('-model', '')
                    if(oncheckup.accessories[i].name == name) {
                      $(this).val(oncheckup.accessories[i].model)
                      $(this).prop('disabled', false)
                    };
                })

                $('input[type="checkbox"]').each(function( index ) {
                    var name = $(this).prop('name')
                    if(oncheckup.accessories[i].name == name) {
                      $(this).prop('checked', true);
                    }
                })
            }

            for(var i=0; i<oncheckup.mechanical.length;i++)
            {
                 $('input[type="text"]').each(function( index ) {
                    var name = $(this).prop('name')
                    name = name.replace('-model', '')
                    if(oncheckup.mechanical[i].name == name) {
                      $(this).val(oncheckup.mechanical[i].model);
                      $(this).prop('disabled', false)
                    }
                })
                 $('input[type="checkbox"]').each(function( index ) {
                    var name = $(this).prop('name')
                    if(oncheckup.mechanical[i].name == name) {
                      $(this).prop('checked', true);
                      var grandparent = $(this).parent().parent();
                      grandparent.children().eq(1).find('select').prop('disabled', false);     
                    }
                })


            }

            for(var i=0; i<oncheckup.dynamic.length;i++)
            {
                 $('input[type="text"]').each(function( index ) {
                    var name = $(this).prop('name')
                    name = name.replace('-model', '')
                    if(oncheckup.dynamic[i].name == name) {
                      $(this).val(oncheckup.dynamic[i].model);
                      $(this).prop('disabled', false)
                    }
                })
                 $('input[type="checkbox"]').each(function( index ) {
                    var name = $(this).prop('name')
                    if(oncheckup.dynamic[i].name == name) $(this).prop('checked', true);
                })
            }

            for(var i=0; i<oncheckup.misc.length;i++){
                $('#misc').find('.card-body .miscinputs').append('<input class="form-control" type="text" placeholder="Input Miscelleneous here" name="misc-' + (i+1) + '" value="'+oncheckup.misc[0].name+'">');
            }
            
            //BEFORE MTR

            $('input[name="beftestingwoload1"]').val(oncheckup.beftestingwoload1)
            $('input[name="beftestingwoload2"]').val(oncheckup.beftestingwoload2)
            $('input[name="beftestingwoload3"]').val(oncheckup.beftestingwoload3)
            $('input[name="beftestingwload1"]').val(oncheckup.beftestingwload1)
            $('input[name="beftestingwload2"]').val(oncheckup.beftestingwload2)
            $('input[name="beftestingwload3"]').val(oncheckup.beftestingwload3)

            $('input[name="befwinding"]').each(function(index){
                $(this).val(oncheckup.before.befwinding[index])
            })

            $('input[name="befinsulation1"]').val(oncheckup.before.befinsulation1)
            $('input[name="befinsulation2"]').val(oncheckup.before.befinsulation2)

            $('input[name="befpi"]').val(oncheckup.before.befpi)

            $('input[name="befhighpotential"]').each(function(index){
                $(this).val(oncheckup.before.befhighpotential[index])
            })

            if(typeof oncheckup.before.befphase1 == 'string')
            {
                $('input[name="befphase1"]').each(function(index){
                    if($(this).val() == oncheckup.before.befphase1)
                        $(this).prop('checked', true)    
                })   
            }
            else if(typeof oncheckup.before.befphase1 == 'object')
            {
                $('input[name="befphase1"]').each(function(index){
                    for(var i=0; i<oncheckup.before.befphase1.length; i++)
                        if($(this).val() == oncheckup.before.befphase1[i]) 
                            $(this).prop('checked', true)
                })
            }

            if(typeof oncheckup.before.befphase2 == 'string') $('input[name="befphase2"]').prop('checked', true)

            if(typeof oncheckup.before.befrewindrecon == 'string') $('input[value="'+oncheckup.before.befrewindrecon+'"]').prop('checked', true)
            $('textarea[name="befremarks"]').val(oncheckup.before.befremarks)
            $('textarea[name="befnotes"]').val(oncheckup.before.befnotes)

            //MOD - ACCESSORIES All select types
            for(var i=0; i<oncheckup.accessories.length; i++)
            {
              if(  
                  oncheckup.accessories[i].name == 'acce-options-capacitor' 
                || oncheckup.accessories[i].name == 'acce-options-fanblade' 
                || oncheckup.accessories[i].name == 'acce-options-oilseal'
                || oncheckup.accessories[i].name == 'acce-options-shaftseal'
                || oncheckup.accessories[i].name == 'acce-options-oring-or-packing'
                || oncheckup.accessories[i].name == 'acce-options-capilla-or-gear'
              )
              {
                
                var selectobjname = oncheckup.accessories[i].name
                var children1 = $('select[name="'+selectobjname+'1"]').children()
                for(var j=0; j<children1.length; j++)
                {
                  if($(children1[j]).val() == oncheckup.accessories[i][selectobjname+'1'])
                    $(children1[j]).prop('selected', true)
                }

                if($('select[name="'+selectobjname+'2"]').children())
                {
                  var children2 = $('select[name="'+selectobjname+'2"]').children()
                  for(var j=0; j<children2.length; j++)
                  {
                    if($(children2[j]).val() == oncheckup.accessories[i][selectobjname+'2'])
                      $(children2[j]).prop('selected', true)
                  }
                }
                
              }
              else if(oncheckup.accessories[i].name == 'acce-other-1')
              {
                  $('input[name=acce-other-1-check]').prop('checked', true)
                  $('input[name=acce-other-1]').val(oncheckup.accessories[i].formal)
                  $('input[name=acce-other-1-model]').val(oncheckup.accessories[i].model)
              }

            }

            //MOD - MECHANICAL All select types
            for(var i=0; i<oncheckup.mechanical.length; i++)
            {
              if(oncheckup.mechanical[i].name == 'acce-reboring-housing' || oncheckup.mechanical[i].name == 'acce-sleeving')
              {
                var selectobjname = oncheckup.mechanical[i].name
                var children = $('select[name="'+selectobjname+'-model"]').children()
                for(var j=0; j<children.length; j++)
                {
                  if($(children[j]).val() == oncheckup.mechanical[i].model)
                  {
                      
                      $(children[j]).prop('selected', true)
                  }
                    
                }
              }
            }

            //MOD - DYNAMIC All select types
            for(var i=0; i<oncheckup.dynamic.length; i++)
            {
              if(oncheckup.dynamic[i].name == 'dynamic-options')
              {
                var selectobjname = oncheckup.dynamic[i].name
                var children = $('select[name="'+selectobjname+'2"]').children()
                for(var j=0; j<children.length; j++)
                {
                  if($(children[j]).val() == oncheckup.dynamic[i][selectobjname+'2'])
                  {
                      
                      $(children[j]).prop('selected', true)
                  }
                    
                }
              }
            }    

          //DELETE
          $('#delete').on('click', function(){
            var answer = window.confirm("Are you sure you want to delete this data? Note: It will delete all its succeeding stages but the preceeding will be retained");
            if (answer) {
                $.ajax({   
                    async: false,  
