    script. 
      //console.log(!{JSON.stringify(motorobj.others)});
      - $("#salesRep option").each(function(){
        
        if ($(this).text() == '#{motorobj.salesRep}')
          $(this).attr("selected","selected");
      });

      - $("select[name='company'] option").each(function(){
        if ($(this).text() == '#{motorobj.company}')
          $(this).attr("selected","selected");
      });

      - $("select[name='motortype'] option").each(function(){
        if ($(this).text() == '#{motorobj.motorType}')
          $(this).attr("selected","selected");
      });

      var powerlist = '#{motorobj.power}'.split(' ');
      - $("input[name='powerrb']").each(function(){
        if(powerlist[1] == $(this).val().toUpperCase()){
          
          $(this).attr('checked', 'checked');
        }
      });

      const checktable = [
        { name: 'Motor Assy', idname: 'motor-assy' },
        { name: 'AC ASSY', idname: 'ac-assy' },
        { name: 'Fabrication', idname: 'fabrication' },
        { name: 'Machining', idname: 'machining' }
      ];

      const checktable2 = [
        { name: 'Fan Cover', idname: 'fan-cover' },
        { name: 'Stator', idname: 'stator' },
        { name: 'Keystock (CUÑA)', idname: 'keystock' },
        { name: 'Fan Blade', idname: 'fan-blade' },
        { name: 'Rotor', idname: 'rotor' },
        { name: 'Centrifugal', idname: 'centrifugal' },
        { name: 'Terminal Box', idname: 'terminal-box' },
        { name: 'Pulley', idname: 'pulley' },
        { name: 'Governor-Switch', idname: 'governor-switch' },
        { name: 'Terminal', idname: 'terminal' },
        { name: 'Carbon', idname: 'carbon' },
        { name: 'Capacitor', idname: 'capacitor' },
        { name: 'Terminal Block', idname: 'terminal-block' },
        { name: 'Carbon Holder', idname: 'carbon-holder' },
        { name: 'Impeller', idname: 'impeller' },
        { name: 'Bolts and Nuts', idname: 'bolts-and-nuts' },
        { name: 'Coupling', idname: 'coupling' }
      ];

      //Others
      var ctr = 0;
      - $.each(!{JSON.stringify(motorobj.others)}, function(){
        if(checktable[ctr]['name'] == $(this)[0]['name']){
          if($(this)[0]['isChecked']){
              $('input[name='+ checktable[ctr]['idname'] +']').attr('checked', 'checked');
          }
        }
        ctr++;
      });

      //Parts
      ctr = 0;
      - $.each(!{JSON.stringify(motorobj.parts)}, function(){
        if(checktable2[ctr]['name'] == $(this)[0]['name']){
          if($(this)[0]['isChecked']){
              $('input[name='+ checktable2[ctr]['idname'] +']').attr('checked', 'checked');
          }
        }
        ctr++;
      })

      //PM 
      if(!{JSON.stringify(motorobj.pmID)}){
        $('.pmcheck[value="pmexist"]').attr('checked', 'checked');
        $('.pmcontent').append('<div class="col-md-6 pmexist"><label > Available PM</label><select class="form-control " name="pmcode"><option>'+!{JSON.stringify(motorobj.pmID)}+'</option></select></div></div>');
      }

      



      

     



     
